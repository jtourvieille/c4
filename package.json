{
  "name": "c4",
  "version": "0.0.1",
  "private": true,
  "description": "c4 sample",
  "type": "module",
  "packageManager": "pnpm@10.7.1",
  "scripts": {
    "start": "likec4 start",
    "dev": "likec4 start",
    "build": "likec4 build",
    "test": "vitest run",
    "postinstall": "npm run generate:model",
    "generate:model": "likec4 gen model -o ./test/likec4-model.ts",
    "generate:relationships-csv": "likec4 gen relationships-csv && open ./relationships.csv",
    "generate:react": "likec4 generate react -o ./react/likec4-views.mjs",
    "export:png": "likec4 export png -o ./png"
  },
  "devDependencies": {
    "@types/node": "^22.18.6",
    "likec4": "latest",
    "vitest": "latest",
    "typescript": "^5.9.3"
  },
  "stackblitz": {
    "installDependencies": true
  }
}
